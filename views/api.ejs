                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <h3>URL</h3>

                        <p id="url-voo-get">
                            <script> document.getElementById('url-voo-get').innerHTML = 'GET http://' + location.host + '/voo'; </script>
                        </p>
                        <h3>Parâmetros - Request</h3>

                        <p>Nenhum</p>

                        <h3>Retorno - Response</h3>

                        <p>
                            <code>
                                [{"_id":"57b47349060844ec078fb4fe", "origem":"Aqaba, JO - Nuweiba, EG", "destino":"Egan,
                                SD, Estados Unidos", "assentos":60, "dataOrigem":"2016-08-17T15:00:00.000Z",
                                "dataDestino":"2016-08-18T15:00:00.000Z", "precoBasico":200, "precoExecutivo":270,
                                "aberto":true, "__v":0}]
                            </code>
                        </p>
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
